                <prj:centre system="J2000" type="RELATIVE">
                    <val:longitude unit="arcsec">0.0</val:longitude>
                    <val:latitude unit="arcsec">0.0</val:latitude>
                    <val:fieldName>DR21-OH-1</val:fieldName>

                <prj:centre system="J2000" type="RELATIVE">
                    <val:longitude unit="arcsec">0.6000639115963191</val:longitude>
                    <val:latitude unit="arcsec">-9.800048873957975</val:latitude>
                    <val:fieldName>Sgr_D-3</val:fieldName>
